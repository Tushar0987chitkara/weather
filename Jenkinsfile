pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'Building'
                echo 'build completed'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing'
                echo 'testing completed'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying'
                echo 'deployment completed'
            }
        }
    }
}
